{
  "name": "cosmic-expansion",
  "version": "0.1.0-dev",
  "description": "Cosmological calculations for the expansion of the universe",
  "files": [
    "dist",
    "types"
  ],
  "type": "module",
  "browser": "dist/cosmic-expansion.min.js",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "types",
  "scripts": {
    "build": "rimraf dist types && rollup -c && tsc --project tsconfig.types.json && npm run test:dist",
    "ci": "npm run lint && npm run test:unit && npm run build",
    "docs": "typedoc",
    "lint": "prettier . --check && eslint .",
    "lint:fix": "prettier . --write && eslint . --fix",
    "test": "npm run lint:fix && npm run test:unit",
    "test:dist": "mocha test/dist/**/*.{spec,test}.{{c,m,}js,ts}",
    "test:unit": "mocha test/unit/**/*.spec.{{c,m,}js,ts}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pb-uk/cosmic-expansion.git"
  },
  "keywords": [
    "calculator",
    "cosmic",
    "cosmology",
    "expansion",
    "inflation",
    "lcdm",
    "physics"
  ],
  "author": "https://github.com/pb-uk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pb-uk/cosmic-expansion/issues"
  },
  "homepage": "https://pb-uk.github.io/cosmic-expansion",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "camelcase": "^7.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-plugin-mocha": "^10.0.4",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@rec-math/math": "^1.1.0"
  }
}
